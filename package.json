{
  "name": "wamworksheet",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/cargo.icns --prune=true --out=release-builds",
   "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/cargo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Wam Sheet\"",    
   "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/cargo.png --prune=true --out=release-builds"
  },
  "author": "Jonathan Kuhl",
  "license": "ISC",
  "dependencies": {
    "electron": "^18.3.11"
  }
}
